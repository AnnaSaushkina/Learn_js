{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [ 
        "airbnb-base",

        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    // Здесь мы подключаем используемые плагины.
    "plugins": [
        "prettier"
    ],
    "rules": {
        "prettier/prettier": "error"
    }
}